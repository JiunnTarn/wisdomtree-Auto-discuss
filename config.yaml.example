# [必要] 课程 id，url 里有
courseId: 0000000000

# [必要] url 里有
recruitId: 000000

# [必要] 问题来源："top" -> 热门问题，"latest" -> 最新问题
questionChannel: "top"

# [必要] 从第几页开始回答（一页50题）
from: 0

# [必要] 回答到第几页（含）
to: 0

# [必要] OpenAI 的 API Key / 自定义 API 提供商的 Key
apiKey: "xxxxxxxxxxxxxxxxx"

# [可选] 自定义 API 域名
# apiHost: "https://xxxxxxxxx"

# [可选] 智慧树 cookie 里有
# wisdomtreeJtCas: "xxxxxxxxxxxxxxxxx"

# [可选] 其他智慧树账户的 jt-cas，用于点赞
# viceWisdomtreeJtCasList:
#   - "xxxxxxxxxxxxxxxxx"

# [可选] 代理
# proxy: {
#   host: "127.0.0.1",
#   port: 0000,
#   auth: {
#       username: "xxxxxx",
#       password: "Pa$sW0Rd"
#   },
#   protocol: "http"
# }

# [可选] 回答间隔时长（毫秒）
# interval: 20000
